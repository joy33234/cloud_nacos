"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.okx.mapper.OkxAccountMapper">

    <resultMap type="com.ruoyi.okx.domain.OkxAccount" id="AccountResult">
